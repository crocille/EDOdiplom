<Page x:Class="EDODiplom.Pages.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EDODiplom.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenuPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" HorizontalAlignment="Left" Width="270">
            <Button Content="Договоры" Click="BtContractClick" HorizontalAlignment="Left"/>
            <Button Content="Материалы" Click="BtMaterialCLick" HorizontalAlignment="Left"/>
            <Button Content="Поставщики" Click="BtSupplierClick" HorizontalAlignment="Left"/>
            <Button Content="Поставка" Click="BtSupplyClick" HorizontalAlignment="Left"/>
            <Button Content="Строительные объекты" Click="BtBuldObjectClick" HorizontalAlignment="Left"/>
            <Button Content="Документация на объекты" Click="BtObjectDocument" HorizontalAlignment="Left"/>
        </StackPanel>
    </Grid>
</Page>
